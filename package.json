{
  "name": "next_app_template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:tailwind": "tailwindcss build -i styles/tailwind.css -o styles/tailwind-styles.css --watch",
    "build:tailwind": "tailwindcss build -i styles/tailwind.css -o styles/tailwind-styles.css",
    "predev": "npm run build:tailwind",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier '{,!(node_modules|.next)/**/}*.js' --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "gray-matter": "^4.0.3",
    "next": "^12.2.1",
    "next-mdx-remote": "^4.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^18.0.3",
    "@types/react": "18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.5",
    "typescript": "4.7.4"
  }
}
